{
  "name": "bunder",
  "description": "Bunder is a package manager for Linux",
  "version": "0.1.0",
  "author": "Joe Hopkins",
  "bin": {
    "bunder": "./bin/run"
  },
  "bugs": "https://github.com/joehop67/bunder/issues",
  "dependencies": {
    "@oclif/command": "^1.5.15",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0",
    "decompress": "^4.2.0",
    "decompress-tar": "^4.1.1",
    "decompress-tarbz2": "^4.1.1",
    "decompress-targz": "^4.1.1",
    "decompress-tarxz": "^2.1.1",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "globby": "^8.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/joehop67/bunder",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
		"hooks": {
			"init": "./src/hooks/init/bootstrap"
		},
    "bin": "bunder",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "joehop67/bunder",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
